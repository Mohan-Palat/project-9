<h1>Edit <%= foundSnack.title %></h1>
<h4>Edit Food Package</h4>
<form action="/snacks/<%= foundSnack.id %>?_method=PUT" method="POST">
  <div class="form-group">
    <label for="name">Food Name</label>
    <input
      type="text"
      class="form-control"
      id="name"
      name="name"
      value="<%= foundSnack.name %>"
    />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>